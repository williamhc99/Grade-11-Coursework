class While3 {
  public static void main(String args[]) {     
    int num = 0;
    while ((num>=0)&&(num<100)){
      System.out.println(num);
      num++;
    }while ((num>=0)&&(num<=100)){
      System.out.println(num);
      num+-;
    }
  }
}