import java.util.Scanner;
class Do7 {
  public static void main(String args[]) {     
    Scanner myScanner = new Scanner(System.in);
    int num = 1;
    do {
      System.out.println(num+" William Chen");
      num++;
    }while((num>0)&&(num<11));
  }
}